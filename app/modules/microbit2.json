{
    "id": "220301",
    "name": {
        "en": "Microbit (V1 + V2)",
        "ko": "마이크로비트"
    },
    "category": "board",
    "platform": ["win32", "darwin"],
    "icon": "microbit2.png",
    "module": "microbit2.js",
    "url": "https://microbit.org/ko/",
    "selectPort": true,
    "reconnect": true,
    "tryFlasherNumber": 2,
    "firmware": [
        {
            "type": "copy",
            "name": "microbit_compatible_lite",
            "afterDelay": 12000,
            "translate": "Microbit v1"
        },
        {
            "type": "copy",
            "name": "microbit2_lite",
            "afterDelay": 7000,
            "translate": "Microbit v2"
        }
    ],
    "driver": {
        "win32-ia32": "mbedWinSerial/mbedWinSerial_16466.exe",
        "win32-x64": "mbedWinSerial/mbedWinSerial_16466.exe"
    },
    "hardware": {
        "type": "serial",
        "control": "slave",
        "delimiter": "\r\n",
        "duration": 128,
        "vendor": "",
        "baudRate": 115200,
        "commType": "ascii"
    }
}
